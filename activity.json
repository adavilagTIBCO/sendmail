{
  "name": "sendmail",
  "version": "0.0.1",
  "type": "flogo:activity",
  "description": "Flogo custom activity to send an email using gmail",
  "author": "TIBCOmpadres team <adavilag@tibco.com>, <aosuna@tibco.com>, <dchavarr@tibco.com> ",
  "ref": "github.com/adavilagTIBCO/sendmail",
  "inputs":[
    {
      "name": "to",
      "type": "string"
    },
    {
      "name": "from",
      "type": "string"
    },
    {
      "name": "password",
      "type": "string"
    },
    {
      "name": "subject",
      "type": "string"
    },
    {
      "name": "message",
      "type": "string"
    },
    {
      "name": "server",
      "type": "string"
    },
    {
      "name": "port",
      "type": "int"
    }

  ],
  "outputs": [
    {
      "name": "result",
      "type": "string"
    }
  ]
}